{
  "name": "electronify",
  "description": "Electron wrapper for websites",
  "version": "0.1.0",
  "main": "./bin/electronify.js",
  "bin": {
    "electronify": "./bin/electronify.js"
  },
  "scripts": {
    "install:prod": "yarn install --offline",
    "postinstall": "electron-builder install-app-deps",
    "start": "node --harmony ./bin/electronify.js",
    "start:dev": "ts-node ./bin/electronify.js",
    "compile": "rm -rf ./built && yarn run tsc -p ./tsconfig.json && cp -r ./offline-cache ./built/ && cp ./yarn.lock ./built/",
    "compile:watch": "rm -rf ./built && yarn run tsc -w -p ./tsconfig.json && cp -r ./offline-cache ./built/ && cp ./yarn.lock ./built/"
  },
  "repository": "https://github.com/UweStolz/electronify",
  "author": "Uwe Stolz <example@mail.com>",
  "license": "UNLICENSED",
  "private": true,
  
  "devDependencies": {
    "inquirer": "^7.0.0",
    "pino": "^5.13.4",
    "pino-pretty": "^3.2.2",
    "yargs": "^14.2.0",
    "@types/fs-extra": "^8.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.7.12",
    "@types/pino": "^5.8.12",
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "ts-node": "^8.4.1",
    "tslib": "^1.10.0",
    "typescript": "^3.6.4"
  }
}
