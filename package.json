{
  "name": "electronify",
  "description": "Electron wrapper for websites",
  "version": "0.1.0",
  "main": "./bin/electronify.js",
  "bin": {
    "electronify": "./bin/electronify.js"
  },
  "scripts": {
    "lint": "yarn run eslint --ext .ts --ext .js .",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "install:prod": "yarn install --offline",
    "postinstall": "electron-builder install-app-deps",
    "start": "node --harmony ./bin/electronify.js",
    "start:dev": "ts-node ./bin/electronify.js",
    "copy-lock-files": "yarn run ncp ./yarn.lock ./built/yarn.lock && yarn run ncp ./app/yarn.lock ./built/app/yarn.lock",
    "compile": "yarn run rimraf ./built && yarn run tsc -p ./tsconfig.build.json && yarn copy-lock-files",
    "compile:watch": "yarn run tsc -w -p ./tsconfig.build.json"
  },
  "repository": "https://github.com/UweStolz/electronify",
  "author": "Uwe Stolz <example@mail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "electron": "^7.1.8",
    "electron-builder": "^22.5.1",
    "image-size": "^0.8.3",
    "inquirer": "^7.1.0",
    "inquirer-fuzzy-path": "^2.2.0",
    "page-icon-finder": "^1.1.0",
    "pino": "^6.2.1",
    "pino-pretty": "^4.0.0",
    "sharp": "^0.25.2",
    "tslib": "^1.11.1",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.1",
    "@types/lodash.capitalize": "4.2.6",
    "@types/lodash.invoke": "4.5.6",
    "@types/node": "^13.13.2",
    "@types/pino": "^6.0.0",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "fs-extra": "^9.0.0",
    "jest": "^25.4.0",
    "ncp": "^2.0.0",
    "ora": "^4.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  }
}
