{
  "name": "electronify",
  "description": "Electron wrapper for websites",
  "version": "0.1.0",
  "bin": {
    "electronify": "./bin/index.js"
  },
  "scripts": {
    "install:prod": "yarn install --production",
    "postinstall": "electron-builder install-app-deps",
    "electronify": "node --harmony ./bin/index.js",
    "electronify:dev": "ts-node ./bin/index.js",
    "compile": "rm -rf ./built && yarn run tsc -p ./tsconfig.json && cp -r ./offline-cache ./built/",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/UweStolz/electronify",
  "author": "Uwe Stolz <example@mail.com>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "inquirer": "^7.0.0",
    "pino": "^5.13.4",
    "pino-pretty": "^3.2.2",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.7.12",
    "@types/pino": "^5.8.12",
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "typescript": "^3.6.4",
    "tslib": "^1.10.0",
    "ts-node": "^8.4.1"
  }
}
